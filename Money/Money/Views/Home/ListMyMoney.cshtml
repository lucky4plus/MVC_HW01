@model Money.Models.ViewModels.MoneyViewModel

@{
    ViewBag.Title = "ListMyMoney";
    Layout = null;
}
<table class="table table-bordered table-hover">
    <tr>
        <th>#</th>
        <th>類別</th>
        <th>日期</th>
        <th>金額</th>
    </tr>
    @{
        for (int i = 0; i < 50; i++)
        {
            <tr>
                <td>@(i + 1)</td>
                <td>@(Enum.GetName(typeof(Money.Models.ViewModels.MyCategory), Model.Category))</td>
                <td>@Model.Date.ToString("yyyy-MM-dd")</td>
                <td>@Model.Money.ToString("N0")</td>
            </tr>
        }
    }

</table>